mixin sOrganize(data)
	// start sOrganize
	+b.SECTION.sOrganize.section#sOrganize.wow.animate__animated.animate__fadeIn&attributes(attributes)
		+e.container.container
			img(src="img/sOrganize-img-1.png", alt="").img
			+b.section-title.text-center
				h3 #[span Профессионально организуем] дорожное движение и&nbsp;получим все погласования &laquo;под ключ&raquo;:
			+e.row.row
				mixin item(text)
					+e.item
						+e.icon-wrap
							+icon('check')
						p!= text
				.col-md-6.col-lg-4
					+item('При прокладке<br>инженерных сетей')
				.col-md-6.col-lg-4
					+item('Для АЗС и&nbsp;объектов дорожного сервиса')
				.col-md-6.col-lg-4
					+item('При занятии Проезжей части при производстве работ')
				.col-md-6.col-lg-4
					+item('Для жилых комплексов, производственных<br>объектов и&nbsp;частных домов')
				.col-md-6.col-lg-4
					+item('При установке<br>лесов/пешеходной галереи')
				.col-md-6.col-lg-4
					+item('При строительстве/ реконструкции съездов с дорог')
	// end sOrganize